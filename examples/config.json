{
    "state_path": "/var/run/rss_state",

    "outputs": {
        "hipchat": {
            "enabled":  true,
            "token":    "<hipchat token>",
            "room":     "1234567",
            "user":     "feedbot"
        },
        "email": {
            "enabled":  true,
            "server":   "localhost",
            "from":     "feedbot@jwplayer.com",
            "to":       "System Alerts <devops@jwplayer.com>"
        },
        "log": {
            "enabled":  true
        },
        "slack": {
            "enabled":  true,
            "token":    "<slack token>",
            "channel":  "#channel"
        }
    },

